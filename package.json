{
  "name": "web-interview",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "start": "yarn server & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "precommit": "lint-staged",
    "server": "yarn json-server --watch ./data/data.json --port 3010"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --no-semi --write",
      "eslint --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "json-server": "^0.15.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "react-scripts": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
